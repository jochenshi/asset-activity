.info{
  .detail_wrapper{
    display flex
    margin 30px
    border 1px solid #e3e3e3
    border-radius 5px
    & > div{
      flex 1
      text-align center
      font-size 16px
      line-height 32px
      padding 10px 0
      &:first-child{
        border-right 1px solid #e3e3e3
      }
      .label{
        margin-right 10px
        font-weight bold
      }
      .num{
        color #8cc63f
      }
    }
  }
}